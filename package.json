{
  "name": "rn-images-assets",
  "version": "0.1.0",
  "repository": "https://github.com/adamTrz/rn-images-assets.git",
  "author": "Adam Trzcinski<trzcinski.adam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src/ -d lib/",
    "precommit": "lint-staged",
    "postbuild": "cp ./src/Contents.json ./lib/Contents.json",
    "prepublish": "npm run build",
    "flow": "flow",
    "lint": "eslint src",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "test": "mocha ./tests/*.js"
  },
  "bin": {
    "rn-images-assets": "./bin/images.js"
  },
  "main": "./lib/index.js",
  "files": [
    "lib/",
    "bin/"
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "mocha": "^4.0.1",
    "mock-fs": "^4.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.8.0",
    "eslint-config-callstack-io": "^0.5.0",
    "flow-bin": "^0.57.2",
    "lint-staged": "^5.0.0",
    "prettier": "^1.7.4"
  },
  "eslintConfig": {
    "extends": "callstack-io",
    "env": {
      "mocha": true
    }
  },
  "babel": {
    "presets": [
      "flow"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote --write './src/**/*.js'",
      "git add"
    ]
  }
}
